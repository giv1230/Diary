@model $safeprojectname$.Models.MType

@{
    ViewData["Title"] = "עריכת סוג פגישה";
}

    <h1>עריכת סוג פגישה</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="MeetingType" class="control-label"></label>
                <input asp-for="MeetingType" class="form-control" />
                <span asp-validation-for="MeetingType" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="שמור" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">חזור לרשימה</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
